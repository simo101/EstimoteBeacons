<?xml version="1.0" encoding="utf-8"?>
<widget id="BeaconRangeNotifier.widget.BeaconRangeNotifier" needsEntityContext="false" offlineCapable="true" xmlns="http://www.mendix.com/widget/1.0/">
    <name>BeaconRangeNotifier</name>
    <description>Notifies the server if a beacon is within range</description>
    <icon></icon>
    <properties>
        <property key="rangeString" type="string" required="true" defaultValue="3.0">
            <caption>Range</caption>
            <category>Behavior</category>
            <description>Within what range should beacpons be detected? Meters</description>
        </property>
        <property key="endPoint" type="string" required="true" defaultValue="beacons">
            <caption>Endpoint</caption>
            <category>Behavior</category>
            <description>This endpoint will be used to do a GET request to the server.</description>
        </property>
        <property key="scanInterval" type="integer" required="true" defaultValue="5000">
            <caption>Scan interval</caption>
            <category>Behavior</category>
            <description>Interval in ms between scans</description>
        </property>
        <property key="beaconSet" type="object" isList="true" required="true">
			<caption>Beacons</caption>
			<category>Beacons</category>
			<description></description>
			<properties>
				<property key="beaconUUID" type="string" required="true" defaultValue="B9407F30-F5F8-466E-AFF9-25556B57FE6D">
					<caption>UUID</caption>
					<category>Beacon</category>
					<description></description>
				</property>
				<property key="beaconMajor" type="integer" required="true" defaultValue="00000">
					<caption>Major</caption>
                    <category>Beacon</category>
					<description></description>
				</property>
				<property key="beaconMinor" type="integer" required="true" defaultValue="00000">
					<caption>Minor</caption>
                    <category>Beacon</category>
					<description></description>
				</property>
			</properties>
		</property>
    </properties>
</widget>
